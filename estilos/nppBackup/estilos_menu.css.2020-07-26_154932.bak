/*INICIO DE CABECERA*/

.cabecera{
	background:#FFF;
	height:75px;
	box-sizing:border-box;
	border-bottom:1px solid #E5E7E9;
	font-family:roboto;
	width:100%;
	display:flex;
	flex-wrap:wrap;
	justify-content:space-between;
	align-items:center; 
	flex-direction:row;
}

.subcontent_1{
	display:flex;
	align-items:center;
}

.cabecera .icon-menu{
	font-size:18px;
	color:#7F8C8D;
	display:inline-block;
	padding:20px 3px;
	
}

.cabecera .logo{
	padding:12px 3px 12px 90px;
	display:inline-block;
	width:145px;
	height:50px;
}

.cabecera .logo img{
	width:145px;
	height:50px;
}



.cabecera .icon-search{
	margin-left:13px;
	display:inline-block;
}

.cabecera  input[type="search"]{

  margin:20px 0px 20px 15px;
  border:1px solid #E5E7E9;
  display:inline-block;
}

.cabecera .user{
    margin:10px 42px;
    display:inline-block;
}

.cabecera .a_user{
    width:20px;
}

.cabecera .user img{
    width:50px;
}




/* FIN DE CABECERA */

